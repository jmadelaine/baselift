{
  "name": "baselift",
  "version": "0.0.7",
  "description": "React base components that implement Emotion",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "sideEffects": false,
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "yarn build",
    "prepublishOnly": "yarn lint"
  },
  "keywords": [
    "base",
    "components",
    "functional",
    "presentational",
    "react",
    "stateless",
    "ui"
  ],
  "author": "Jonathan Madelaine",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@emotion/core": "^10.0.27",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "tslint": "^6.0.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.7.5"
  },
  "peerDependencies": {
    "@emotion/core": "^10.0.22"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jmadelaine/baselift.git"
  }
}
