{
  "name": "baselift",
  "version": "0.2.0",
  "description": "React base components that implement Emotion",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "sideEffects": false,
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.tsx\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "yarn build",
    "prepublishOnly": "yarn format && yarn lint && yarn test",
    "test": "jest --config jestconfig.json"
  },
  "keywords": [
    "base",
    "components",
    "functional",
    "presentational",
    "react",
    "stateless",
    "ui"
  ],
  "author": "Jonathan Madelaine",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@emotion/core": "^10.0.28",
    "@testing-library/react": "^10.0.3",
    "@types/jest": "^25.2.1",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.6",
    "@types/react-test-renderer": "^16.9.2",
    "codecov": "^3.6.5",
    "jest": "^25.4.0",
    "jest-emotion": "^10.0.32",
    "prettier": "^2.0.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-test-renderer": "^16.13.1",
    "ts-jest": "^25.4.0",
    "tslint": "^6.1.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.8.3"
  },
  "peerDependencies": {
    "@emotion/core": "^10.0.28"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jmadelaine/baselift.git"
  }
}
